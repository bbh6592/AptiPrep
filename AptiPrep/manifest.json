{
    "manifest_version": 3,
    "name": "AptiPrep Helper",
    "version": "1.0",
    "description": "A simple tool to provide study resources for aptitude.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [{
      "resources": ["progress.html"],
      "matches": ["<all_urls>"]
    }]
  }
  